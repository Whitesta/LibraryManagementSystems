@using LibraryManagementSystems.Models.Entity
@model List<Books>
@{
    ViewBag.Title = "AuthorBooks";
    Layout = "~/Views/Shared/Layout.cshtml";
}

<h2>Author the Books: @ViewBag.Carry</h2>
<br />
<table class="table table-striped">
    <tr>
        <th>Id</th>
        <th>BookName</th>
        <th>Category</th>
        <th>Publisher</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.BookName</td>
            <td>@item.Categories.CategoryName</td>
            <td>@item.Publisher</td>
        </tr>
    }
</table>

